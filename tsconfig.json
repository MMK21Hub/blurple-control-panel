{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["ES2021"],
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "strict": true
  },
  "exclude": ["node_modules"]
}
